--if(_G.BETATESTING==true)and(_G.envirement==true)and(_G.outdatedversion==false)then
loadstring(game:HttpGet('https://raw.githubusercontent.com/killerbrasko/Hoodshark/main/helloworld'))()		
wait()
local hoodshark = game.CoreGui:WaitForChild("Hoodshark");
local frame = hoodshark:FindFirstChild("Main")
frame.Draggable = true
frame.Active = true
frame.Selectable = true

local sound = Instance.new("Sound")
sound.Parent=game.Workspace
sound.Name="Goodmsg"
sound.Volume=3
sound.SoundId = "rbxassetid://3023237993"

local sound2 = Instance.new("Sound")
sound2.Parent=game.Workspace
sound2.Name="Badmsg"
sound2.Volume=6
sound2.SoundId = "rbxassetid://550209561"

local function clean()
	for i,v in pairs(frame.Pages:GetChildren())do
		if(v):IsA('Frame')or(v):IsA("ScrollingFrame")then
			v.Visible = false
		end
	end
end

local plr = game:GetService'Players'.LocalPlayer
local UserInputService = game:GetService("UserInputService")
local FrameObject = frame
local PositionClosed = UDim2.new(0.304, 0,-1, 0)
local PositionOpen = UDim2.new(0.304, 0,0.27, 0)
local Minimized = true
UserInputService.InputBegan:connect(function(inputObject, gameProcessedEvent)
	if inputObject.KeyCode == Enum.KeyCode.RightControl then
		if Minimized then
			FrameObject:TweenPosition((PositionClosed), 'Out', 'Linear',.25)
			Minimized = false
		else
			Minimized = true
			FrameObject:TweenPosition((PositionOpen), 'Out', 'Linear',.25)
		end
	end
end)
frame.TopBar.Minimize.MouseButton1Click:Connect(function()
	FrameObject:TweenPosition((PositionClosed), 'Out', 'Linear',.25)
end)

frame.SideBar.Combatone.MouseButton1Click:Connect(function()
	if(_G.morphmode==false)then
		clean()
		frame.Pages.Pageone.Visible=true
		frame.Pages.P1SCROLL.Visible=true
		print('loaded combat')
	end
end)

frame.SideBar.Combattwo.MouseButton1Click:Connect(function()
	if(_G.morphmode==false)then
		clean()
		frame.Pages.Pagetwo.Visible=true
	end
end)

frame.SideBar.Settings.MouseButton1Click:Connect(function()
	if(_G.morphmode==false)then
		clean()
		frame.Pages.Pagesetting.Visible=true
	end
end)

frame.SideBar.Waypoints.MouseButton1Click:Connect(function()
	if(_G.morphmode==false)then
		clean()
		frame.Pages.Pagefour.Visible=true
	end
end)

frame.SideBar.Gunmods.MouseButton1Click:Connect(function()
	if(_G.morphmode==false)then
		clean()
		frame.Pages.Pagethree.Visible=true
		frame.Pages.P2SCROLL.Visible=true
	end
end)

frame.SideBar.Client.MouseButton1Click:Connect(function()
	if(_G.morphmode==false)then
		clean()
		frame.Pages.Pageclient.Visible=true
	end
end)

frame.Pages.Pagetwo.Morphs.MouseButton1Click:Connect(function()
	if(frame.Pages.Pagetwo.MorphScroll.Visible==false)then
		frame.Pages.Pagetwo.MorphScroll.Visible=true
	elseif(frame.Pages.Pagetwo.MorphScroll.Visible==true)then
		frame.Pages.Pagetwo.MorphScroll.Visible=false
	end
	end)
print('loaded start end')	
--end	
print('loaded end')
